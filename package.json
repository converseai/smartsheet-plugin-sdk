    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.3",
    "mocha": "^5.1.1",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "sinon-express-mock": "^2.0.4"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "lodash": "^4.17.10"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "commonjs": true
